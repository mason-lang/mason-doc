{
	"private": "true",
	"scripts": {
		"lint": "eslint assets/**/*.es6",
		"serve": "dum serve"
	},
	"devDependencies": {
		"dum": "dum-build/dum",
		"jstransformer-babel": "0.2.0",
		"jstransformer-jade": "0.1.0",
		"jstransformer-stylus": "1.0.0",
		"babel-eslint": "4.1.3",
		"eslint": "1.5.1"
	},
	"dumConfig": {
		"transform": {
			"babel": {
				"modules": "amd",
				"nonStandard": true,
				"whitelist": [
					"es6.destructuring",
					"es6.modules",
					"es6.parameters",
					"asyncToGenerator", "es7.asyncFunctions",
					"es6.properties.shorthand",
					"strict"
				]
			}
		}
	},
	"eslintConfig": {
		"parser": "babel-eslint",
		"rules": {
			"comma-style": [ 1, "last" ],
			"camelcase": 0,
			"curly": [ 1, "multi" ],
			"default-case": 1,
			"generator-star-spacing": [ 1, "after" ],
			"max-len": [ 1, 100, 4 ],
			"new-cap": 0,
			"no-constant-condition": 0,
			"no-extra-parens": 1,
			"no-inner-declarations": 0,
			"no-extend-native": 0,
			"no-fallthrough": 0,
			"no-floating-decimal": 1,
			"no-labels": 0,
			"no-loop-func": 0,
			"no-mixed-requires": 0,
			"no-multiple-empty-lines": [ 1, { "max": 2 } ],
			"no-plusplus": 1,
			"no-shadow": 0,
			"no-throw-literal": 1,
			"no-underscore-dangle": 0,
			"no-unused-vars": 1,
			"no-use-before-define": 0,
			"no-var": 1,
			"no-void": 1,
			"no-warning-comments": 0,
			"operator-assignment": [ 1, "never" ],
			"padded-blocks": [ 1, "never" ],
			"quote-props": [ 1, "as-needed" ],
			"quotes": [ 1, "single" ],
			"semi": 0,
			"space-before-blocks": [ 1, "always" ],
			"space-before-function-paren": [ 1, "never" ],
			"spaced-comment": 1,
			"space-in-parens": [ 1, "never" ]
		}
	},
	"license": "WTFPL"
}